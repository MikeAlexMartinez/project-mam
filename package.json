{
  "name": "project-mam",
  "version": "0.0.1",
  "description": "My personal portfolio website",
  "main": "server.js",
  "scripts": {
    "start": "nodemon -w dist dist/server.js",
    "compile-server": "babel server --presets es2015-node6 --out-dir dist --source-maps",
    "watch-server": "babel server --presets es2015-node6 --out-dir dist --source-maps --watch",
    "start-hook": "nodemon -w server server/start_hook.js",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "dev-all": "npm run watch & npm run watch-server & npm start",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "lint": "eslint --ext js,jsx src server webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeAlexMartinez/project-mam.git"
  },
  "keywords": [
    "node",
    "javascript",
    "react",
    "portfolio"
  ],
  "author": "michael martinez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikeAlexMartinez/project-mam/issues"
  },
  "homepage": "https://github.com/MikeAlexMartinez/project-mam#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "express": "^4.15.4",
    "material-design-icons": "^3.0.1",
    "material-ui": "^1.0.0-beta.6",
    "material-ui-icons": "^1.0.0-beta.5",
    "moment": "^2.18.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.8",
    "mongoose-proptypes": "0.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-sidebar": "^2.3.2",
    "react-slick": "^0.14.11",
    "source-map-support": "^0.4.16",
    "typeface-roboto": "0.0.35",
    "uuid": "^3.1.0",
    "vhost": "^3.0.2",
    "whatwg-fetch": "^2.0.3"
